extends layout
block content
	.container-fluid
		.row
			.col-xs-4
				h1= title
		.row
			.col-xs-4
				p Welcome to #{title}
		.row
			.col-xs-4
				button#edit.btn.btn-primary() edit




		
	//- ul
	//- 	each user, i in userlist
	//- 		li
	//- 			a(href="mailto:#{user.email}")= user.firstName

	.container-fluid		
		table.table-striped.table.table-bordered.table-hover
			tr
				th user
				th dj Status
				th access
				th first Name
				th last Name
				th email
				th phone
				th student Status
				th mac id
				th mac iclass
				th grad year
				th show
				th blurb

			each user, i in userlist
				tr
					td= user._id
					td= user.djStatus
					td.editable= user.access
					td.editable= user.firstName
					td.editable= user.lastName
					td.editable= user.email
					td.editable= user.phone
					td.editable= user.studentStatus
					td.editable= user.macIdNum
					td.editable= user.iclass
					td.editable= user.gradYear
					td.editable= user.show
					td.editable= user.blurb

